<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.<%= options.app.nameSlug  %>" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name><%= options.app.name %></name>
    <description>
        <%= options.app.desc %>
    </description>
    <author email="<%= options.app.email %>" href="<%= options.app.www %>">
        <%= options.app.user %>
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="<%= _.contains(preferences, 'fullscreen') %>" />
    <preference name="webviewbounce" value="<%= _.contains(preferences, 'webviewbounce') %>" />
    <preference name="UIWebViewBounce" value="<%= _.contains(preferences, 'UIWebViewBounce') %>" />
    <preference name="DisallowOverscroll" value="<%= _.contains(preferences, 'DisallowOverscroll') %>" />
</widget>